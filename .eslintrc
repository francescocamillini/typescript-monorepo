{
    "extends": ["plugin:prettier/recommended", "plugin:sonarjs/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier", "sonarjs", "@typescript-eslint"],

    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/no-unused-vars": "error"
    },

    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    }
}
